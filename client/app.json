{
  "expo": {
    "name": "Loop",
    "slug": "LoopApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/loop.png",
    "scheme": "loop",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/loop.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F0F0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/loop.png",
        "backgroundColor": "#F0F0F0"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.reditinere.loop",
      "softwareKeyboardLayoutMode": "adjustResize",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/loop.png"
    },
    "plugins": [
      "expo-notifications",
      "expo-router",
      "expo-font",
      "expo-web-browser",
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }],
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceder a tus fotos para que puedas publicar artículos con imágenes.",
          "cameraPermission": "La app necesita acceder a tu cámara para que puedas tomar fotos y publicar artículos con imágenes."
        }
    ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e15a4c19-7d82-4e86-b049-24d3b4d55f44"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e15a4c19-7d82-4e86-b049-24d3b4d55f44"
    }
  }
}
